<openerp>
<data>
<record id="stock_move_processing_item_view_form" model="ir.ui.view">
    <field name="name">stock.move_processing_item form</field>
    <field name="model">stock.move_processing_item</field>
    <field name="arch" type="xml">
        <form>
            <header/>
            <group name="header" colspan="4" col="2">
                <field name="name"/>
                <field name="product_id"/>
                <field name="active"/>
            </group>
            <notebook>
                <page name="quantity_computation" string="Quantity Computation Code">
                    <field name="quantity_computation_code"/>
                </page>
                <page name="uom_computation" string="UoM Computation Code">
                    <field name="uom_computation_code"/>
                </page>
                <page name="note" string="Note">
                    <field name="note"/>
                </page>
            </notebook>
        </form>
    </field>
</record>

<record id="stock_move_processing_item_view_tree" model="ir.ui.view">
    <field name="name">stock.move_processing_item tree</field>
    <field name="model">stock.move_processing_item</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="product_id"/>
        </tree>
    </field>
</record>

<record id="stock_move_processing_item_action" model="ir.actions.act_window">
    <field name="name">Move Processing Item</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">stock.move_processing_item</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree,form</field>
</record>

<menuitem
    id="stock_move_processing_item_menu"
    name="Stock Move Processing Items"
    parent="stock.menu_stock_configuration"
    action="stock_move_processing_item_action"
    sequence="10"
    />

</data>
</openerp>
