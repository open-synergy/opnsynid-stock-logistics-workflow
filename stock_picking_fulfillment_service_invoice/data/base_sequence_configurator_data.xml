<openerp>
<data>
<!-- Training -->
<record id="base_sequence_stock_fulfillment_service" model="base.sequence_configurator">
    <field
                name="model_id"
                ref="stock_picking_fulfillment_service_invoice.model_stock_fulfillment_service"
            />
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','stock.fulfillment_service'),('name','=','name')]"
            />
    <field
                name="fallback_sequence_id"
                ref="stock_picking_fulfillment_service_invoice.sequence_fulfillment_service"
            />
</record>

<record
            id="base_sequence_stock_fulfillment_service_line"
            model="base.sequence_configurator_line"
        >
    <field
                name="generator_id"
                ref="stock_picking_fulfillment_service_invoice.base_sequence_stock_fulfillment_service"
            />
    <field name="sequence" eval="1" />
    <field
                name="sequence_computation_code"
            >result = document.item_id.sequence_id</field>
</record>
</data>
</openerp>
